{
  "expo": {
    "name": "onewindow",
    "slug": "onewindow",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "onewindow",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "assetBundlePatterns": [
      "assets/fonts/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.onewindow.app",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Your location helps us personalize your experience by automatically setting your country dial code, currency, and other relevant preferences. ",
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "onewindow"
            ]
          }
        ]
      },
      "buildNumber": "12"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.onewindow.app",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "onewindow.io"
            }
          ],
          "category": [
            "DEFAULT",
            "BROWSABLE"
          ]
        }
      ],
      "softwareKeyboardLayoutMode" : "pan",
      "versionCode": 6
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-secure-store",
        {
          "faceIDPermission": "Allow One Window to access your Face ID biometric data."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Your location helps us personalize your experience by automatically setting your country code, currency, and other relevant preferences. "
        }
      ],
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "f99993ed-e6d1-4a64-97bb-5ff255bd8f2f"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/f99993ed-e6d1-4a64-97bb-5ff255bd8f2f"
    }
  }
}
