{
  "cli": {
    "version": ">= 7.8.1"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "iosTest": {
      "autoIncrement": true,
      "channel": "ios_test",
      "distribution": "store"
    },
    "iosProduction":{
      "autoIncrement": true,
      "channel": "ios_production",
      "distribution":"store"
    },
    "iosAdhoc":{
      "autoIncrement": true,
      "channel": "ios_adhoc",
      "distribution":"internal"
    },
    "androidTest":{
      "autoIncrement": true,
      "channel": "android_test",
      "android": {
        "buildType": "apk"
      }
    },
    "androidProduction":{
      "autoIncrement": true,
      "channel": "android_production",
      "android": {
        "buildType": "app-bundle"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
